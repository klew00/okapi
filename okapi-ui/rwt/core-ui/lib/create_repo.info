mvn install:install-file -Dfile=org.eclipse.rap.rwt_1.5.1.20120917-1116_unsigned.jar -DpomFile=rwt-1.5.1.pom -Dpackaging=jar -DlocalRepositoryPath=repo -DcreateChecksum=true
